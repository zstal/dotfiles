[init]
	defaultBranch = develop

[log]
	date = iso

[alias]
	br = branch
	ci = commit
	co = checkout
	lg = log --oneline --graph
	st = status

[core]
	excludesfile = ~/.gitignore_global

[push]
	autoSetupRemote = true

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[include]
	path = ~/.gituser
